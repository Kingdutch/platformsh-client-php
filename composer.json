{
    "name": "platformsh/client",
    "description": "Platform.sh API client",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "cocur/slugify": "^2.0 || ~1.0",
        "guzzlehttp/guzzle": "~5.3",
        "commerceguys/guzzle-oauth2-plugin": "dev-pjcdawkins@dev",
        "guzzlehttp/cache-subscriber": "~0.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pjcdawkins/guzzle-oauth2-plugin"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "Platformsh\\Client\\": "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Platformsh\\Client\\Tests\\": "tests"
      }
    },
    "authors": [
        {
            "name": "Patrick Dawkins"
        }
    ],
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    }
}
