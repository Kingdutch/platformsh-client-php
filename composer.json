{
    "name": "platformsh/client",
    "description": "Platform.sh API client",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "cocur/slugify": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "platformsh/oauth2": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "repositories": [{
        "type": "vcs",
        "url": "git@github.com:pjcdawkins/platformsh-oauth2-php.git"
    }],
    "autoload": {
        "psr-4": {
            "Platformsh\\Client\\": "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Platformsh\\Client\\Tests\\": "tests"
      }
    },
    "authors": [
        {
            "name": "Patrick Dawkins"
        }
    ],
    "config": {
        "platform": {
            "php": "5.6"
        }
    }
}
